{
  "name": "bi-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:db": "cd labelDetector && rethinkdb",
    "dev:dataObject": "cd dataObject && node index.js",
    "dev:labelDetector": "cd labelDetector && node index.js",
    "dev:current": "vite",
    "dev:server": "concurrently \"npm run dev:db\" \"npm run dev:dataObject\" \"npm run dev:labelDetector\"",
    "dev:all": "concurrently \"npm run dev:db\" \"npm run dev:dataObject\" \"npm run dev:labelDetector\" \"npm run dev:current\"",
    "test": "jest --watchAll --verbose"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "vite": "^5.0.8"
  }
}
